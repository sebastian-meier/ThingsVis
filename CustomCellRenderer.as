package {     import fl.controls.listClasses.CellRenderer;     import flash.text.TextFormat;     import flash.filters.BevelFilter;     public class CustomCellRenderer extends CellRenderer {         public function CustomCellRenderer() {             var format:TextFormat = new TextFormat(); 			format.align = 'left'            setStyle("textFormat", format);			setStyle("textPadding", 3);        }     } }